 <div id="page-contents">
    	<div class="container">
    		<div class="row">

          <!-- Newsfeed Common Side Bar Left
          ================================================= -->
    			<div class="col-md-3 static">
          
          {{profile-card img=model.user.photoURL name=model.user.name cover=model.user.coverUrl follower="2089" }}
          
          {{chat-card onlinePepole=model.friend }}
           
            </div>
         
    			<div class="col-md-7">

            <!-- Post Create Box
            ================================================= -->
           
            <!-- Post Create Box End-->

            <h3>Users</h3>
            {{#if searchUser }}
               <div class="friend-list">
                <div class="row">
              {{#each users as |friend|}}
                  <div class="col-md-6 col-sm-6">
                    <div class="friend-card">
                      <img src="{{if friend.cover.url friend.cover.url "assets/images/defaultcover.png"}}" alt="profile-cover" class="img-responsive cover saturation-filter" onerror="this.src='assets/images/defaultcover.png'" />
                      <div class="card-info">
                        <img src="{{friend.photoURL}}" alt="user" class="profile-photo-lg saturation-filter" onerror="this.src='assets/images/defaultUser.png'"/>
                        <div class="friend-info">
                          <a href="#" class="pull-right text-green"></a>
                          <h5><a href="timeline.html" class="profile-link">{{#link-to "profile" (query-params id=friend.id)}}{{friend.name}}{{/link-to}}</a></h5>
                          <!--<p>Student at Harvard</p>
                          -->
                        </div>
                      </div>
                    </div>
                  </div>

                 {{/each}}
                 
                 
                 </div>
                 </div>

             {{else}}
             <div> No User Found </div>
             {{/if}}
            <hr>
            <!-- Post Content
            ================================================= -->
          <h3>Posts</h3>
           {{#if searchData }}
           {{#each arrangedContent as |po|}}
                {{post-card-with-image data=po user=model.user }}
             {{/each}}
             {{else}}
             <div> No Post Found </div>
             {{/if}}
           <!-- End Post Content -->
           </div>

          <!-- Newsfeed Common Side Bar Right
          ================================================= -->
    			<div class="col-md-2 static">
            <div class="suggestions" id="sticky-sidebar">
              <h4 class="grey">Suggestion <p style="font-size: 11px;">( {{ totalSuggestion}} peoples )</p></h4>

                            
              {{#each model.suggest as |user|}}
              <div class="follow-user" >
              
                <img src="{{user.photoURL}}" width="300" class="profile-photo-sm pull-left" />
                <div>
                  <h5>{{#link-to "profile" (query-params id=user.id)}}{{user.name}}{{/link-to}}</h5>
                  <a href="#" class="text-green">Add friend</a>
                </div>
               
              </div>
             
              {{/each}}
            
            </div>
          </div>
    		</div>
    	</div>
    </div>